{
    "name": "swri-sodas/sodas-helper",
    "description": "SodasHelper plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "cakephp/cakephp": "^4.2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "SodasHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SodasHelper\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
